    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_tablemulti"
  },
  "require": {
    "php": "^8.1",
    "ext-pdo": "*",
    "contao-community-alliance/dc-general": "^2.3@dev",
    "contao/core-bundle": "^4.13.0, <5.0",
    "doctrine/dbal": "^3.3.2",
    "metamodels/core": "^2.3@dev",
    "symfony/dependency-injection": "^5.4",
    "symfony/http-kernel": "^5.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "menatwork/contao-multicolumnwizard-bundle": "^3.4",
    "phpcq/all-tasks": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\AttributeTableMultiBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\AttributeTableMultiBundle\\Test\\": "tests/Test"
    }
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\AttributeTableMultiBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-feature/2.3.0": "2.3.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": false,
      "contao/manager-plugin": false
    },
    "sort-packages": true
  }
}
